CC	 = @CC@
SNMP_FLAG = @SNMP_SUPPORT@
VRRP_AUTH_FLAG = @VRRP_AUTH_SUPPORT@
LIBIPTC_FLAG = @USE_LIBIPTC@
LIBIPSET_FLAG = @USE_LIBIPSET@
NL3_FLAG = @USE_NL3@
INCLUDES = -I../include -I../../lib
CFLAGS	 = $(INCLUDES) @CFLAGS@ @CPPFLAGS@ \
	   -Wall -Wunused -Wstrict-prototypes
COMPILE	 = $(CC) $(CFLAGS) @APP_DEFS@

OBJS = 	vrrp_daemon.o vrrp_print.o vrrp_data.o vrrp_parser.o \
	vrrp.o vrrp_notify.o vrrp_scheduler.o vrrp_sync.o vrrp_index.o \
	vrrp_netlink.o vrrp_arp.o vrrp_if.o vrrp_track.o vrrp_ipaddress.o \
	vrrp_iproute.o vrrp_iprule.o vrrp_ndisc.o vrrp_vmac.o \
	vrrp_if_config.o

ifeq ($(VRRP_AUTH_FLAG),_WITH_VRRP_AUTH_)
  OBJS += vrrp_ipsecah.o
endif

ifeq ($(LIBIPTC_FLAG),_HAVE_LIBIPTC_)
  OBJS += vrrp_iptables.o vrrp_iptables_calls.o
endif

ifeq ($(LIBIPSET_FLAG),_HAVE_LIBIPSET_)
  OBJS += vrrp_ipset.o
endif

ifeq ($(SNMP_FLAG),_WITH_SNMP_)
  OBJS += vrrp_snmp.o
endif

HEADERS = $(OBJS:.o=.h)

.c.o:
	$(COMPILE) -c $<

all:	$(OBJS)

clean:
	rm -f *.a *.o *~

distclean: clean
	rm -f Makefile

